<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.eksad.kotlinintro">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-feature android:name="android.permission.camera"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".UpdateDataMahasiswaActivity">
        </activity>
        <activity android:name=".DetailMahasiswaActivity">
        </activity>
        <activity android:name=".InputDataMahasiswaActivity">
        </activity>
        <activity android:name=".DaftarMahasiswaActivity">
        </activity>
        <activity android:name=".VolleyActivity">
        </activity>
        <activity android:name=".EsafirmActivity">
        </activity>
        <activity android:name=".CameraActivity">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".SplashScrennActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>